{
  "name": "web-snippets",
  "displayName": "web-snippets",
  "description": "This extension contains snippets for HTML, CSS and Javascript.",
  "version": "1.1.3",
  "publisher": "UI",
  "icon": "images/web-snippetsIcon.png",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gopi1216/web-snippets.git"
  },
  "contributes": {
    "snippets": [ {
        "language": "html",
        "path": "./snippets/htmlSnippets.json"
      },
      {
        "language": "css",
        "path": "./snippets/cssSnippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/jsSnippets.json"
      }
    ]
  }
}